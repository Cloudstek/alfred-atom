    "change-case": "^3.1.0",
    "color": "^3.0.0",
    "cson-parser": "^4.0.2",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.2",
    "moment": "^2.19.4",
    "sharp": "^0.22.1",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/cson-parser": "^4.0.0",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.0.2",
    "@types/sharp": "^0.22.2",
    "del-cli": "^2.0.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8"
