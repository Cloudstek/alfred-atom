{
  "name": "alfred-atom",
  "version": "2.3.0",
  "description": "Simple workflow that allows you to browse and open Atom projects",
  "dependencies": {
    "alfy": "^0.6.0",
    "fuzzaldrin": "^2.1.0",
    "season": "^5.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2017": "^6.22.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "watch": "babel -w src --out-dir lib",
    "test": "xo && mocha",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup"
  },
  "babel": {
    "comments": false,
    "compact": true,
    "minified": true,
    "presets": [
      "es2017"
    ]
  },
  "xo": {
    "space": 4,
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "no-undef": "off"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mdeboer/alfred-atom.git"
  },
  "author": "Maarten de Boer <maarten@cloudstek.nl>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/mdeboer/alfred-atom/issues"
  },
  "homepage": "https://github.com/mdeboer/alfred-atom#readme"
}
